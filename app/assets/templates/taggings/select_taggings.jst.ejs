<% taggings.forEach(function(tagging, index) { %>
  <div class="tagging">
    <label for="select<%= index %>">
      <input id="select<%= index %>" 
             type="checkbox" 
             name="recipe[tags_attributes][<%= index %>][tagging_id]" 
             value="<%= tagging.id %>"
      >
      <span><%- tagging.get("name") %></span>
    </label>
  </div>
<% }); %>
